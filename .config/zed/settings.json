{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "theme": {
    "mode": "system",
    "light": "Alabaster",
    "dark": "Ayu Dark"
  },
  "ui_font_family": "Zed Plex Sans",
  "ui_font_size": 15,
  "preferred_line_length": 120,
  "tab_size": 2,
  "buffer_font_family": "Iosevka Fixed",
  "buffer_font_size": 15,
  "autosave": "on_focus_change",
  "auto_signature_help": true,
  "wrap_guides": [120],
  "lsp": {
    "metals": {
      "settings": {
        "inlayHints": {
          "inferredTypes": {
            "enable": true
          }
        }
      }
    }
  }
}
